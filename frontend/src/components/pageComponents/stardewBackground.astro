

<script>

    import Starback from "starback";

    let canvas = document.getElementById ("background");

    let starback = new Starback (canvas , {
        type : "dot",
        quantity : 100,
        direction : 200,
        backgroundColor : ["#3c3c3c"],
        randomOpacity : true,
        starColor : ["#ffffff"]
    });

    starback.generateStar(30);
</script>