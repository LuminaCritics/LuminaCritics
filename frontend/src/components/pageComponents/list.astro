---
interface Props {
    title : string;
    category : string;
}

const {title , category} = Astro.props;
---

<div class = "flex flex-col ">
    <button class = "btn btn-ghost flex justify-center text-4xl uppercase underline decoration-blue-500 font-bold pb-5"> {title} </button>
    <div class="h-96 carousel carousel-vertical rounded-box ml-7">
       <slot />
    </div>
</div>